        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Valuuttamuunnin" Height="150" Width="425">
    <Grid Background="#FFE6D311">
        <TextBox x:Name="txtEuro" HorizontalAlignment="Left" Height="23" Margin="50,50,0,0" TextWrapping="Wrap" Text="eurot" VerticalAlignment="Top" Width="120" TextChanged="txtEuro_TextChanged"/>
        <TextBox x:Name="txtDollari" HorizontalAlignment="Left" Height="23" Margin="229,50,0,0" TextWrapping="Wrap" Text="dollarit" VerticalAlignment="Top" Width="120"/>
